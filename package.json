{
  "name": "ttt.io",
  "description": "Antonin Stefanutti's Blog",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "publish": "gatsby build --prefix-paths && rm -rf docs && mv public docs",
    "dev": "gatsby develop",
    "debug": "node --nolazy --inspect-brk node_modules/gatsby/dist/bin/gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "tslint 'src/**/*.{ts,tsx}' -p tsconfig.json",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@mdx-js/mdx": "1.6.14",
    "@mdx-js/react": "1.6.14",
    "@tufte-markdown/remark-figure-parser": "1.0.0",
    "@tufte-markdown/remark-figure-transformer": "1.0.0",
    "@types/mdx-js__react": "1.5.2",
    "@types/plotly.js": "1.50.16",
    "@types/react": "16.9.43",
    "@types/react-helmet": "5.0.14",
    "@types/react-loadable": "5.5.3",
    "@types/react-plotly.js": "2.2.4",
    "@types/styled-components": "4.4.0",
    "d3-sankey": "0.12.3",
    "gatsby": "2.24.8",
    "gatsby-image": "2.4.13",
    "gatsby-plugin-canonical-urls": "2.3.10",
    "gatsby-plugin-feed-mdx": "1.0.1",
    "gatsby-plugin-manifest": "2.4.21",
    "gatsby-plugin-mdx": "1.2.29",
    "gatsby-plugin-react-helmet": "3.3.10",
    "gatsby-plugin-sharp": "2.6.22",
    "gatsby-plugin-styled-components": "3.3.10",
    "gatsby-plugin-typescript": "2.4.16",
    "gatsby-plugin-use-dark-mode": "1.1.2",
    "gatsby-remark-autolink-headers": "2.3.11",
    "gatsby-remark-copy-linked-files": "2.3.11",
    "gatsby-remark-images": "3.3.21",
    "gatsby-remark-katex": "3.3.10",
    "gatsby-remark-smartypants": "2.3.10",
    "gatsby-remark-vscode": "3.0.1",
    "gatsby-source-filesystem": "2.3.23",
    "gatsby-transformer-remark": "2.8.27",
    "gatsby-transformer-sharp": "2.5.11",
    "hast-util-to-html": "3.1.0",
    "katex": "0.12.0",
    "mdast-util-to-hast": "2.5.0",
    "plotly.js": "1.54.7",
    "prettier": "2.0.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "5.2.1",
    "react-loadable": "5.5.0",
    "react-plotly.js": "2.4.0",
    "rimraf": "3.0.2",
    "speakingurl": "14.0.1",
    "styled-components": "4.4.1",
    "tslint": "6.1.2",
    "tslint-config-blvd": "1.2.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "typeface-jetbrains-mono": "2.0.0",
    "typescript": "3.9.7",
    "unist-util-remove": "2.0.0",
